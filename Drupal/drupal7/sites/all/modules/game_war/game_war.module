function game_war_refresh_positions() {
	$sSql  = "select s1.uid, s1.rank, s2.rank2 , s3.rank3 from (SELECT uid, @rownum := @rownum + 1 AS rank FROM (select * from due_user_gamedata order by soldati desc, name) t1, (SELECT @rownum := 0) r1 ) s1, (SELECT uid, @rownum2 := @rownum2 + 1 AS rank2 FROM (select * from due_user_gamedata order by (soldati+bonus_atac) desc) t2, (SELECT @rownum2 := 0) r2 ) s2, (SELECT uid, @rownum3 := @rownum3 + 1 AS rank3 FROM (select * from due_user_gamedata order by (soldati+bonus_aparare) desc) t3, (SELECT @rownum3 := 0) r3 ) s3 where s1.uid=s2.uid and s1.uid=s3.uid ";
	$result = db_query($sSql);
	foreach ($result as $rs) {
		$pg = (int)((($rs->rank - 1) / 30)+1);
		$sSql  = "update {user_gamedata} set pozitie=$rs->rank, pagina=$pg, top_atac=$rs->rank2, top_aparare=$rs->rank3 where uid = " . $rs->uid;
		db_query($sSql);
	}
}

