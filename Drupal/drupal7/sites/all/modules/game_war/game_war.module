/**
 * Implementation of hook_init().
 */
function game_war_init() {
    global $user;
	
	drupal_add_css(drupal_get_path('module', 'game_war') .'/css/game_war.css');
	drupal_add_js(drupal_get_path('module', 'game_war') .'/js/game_war.js');

	drupal_add_css(drupal_get_path('module', 'game_war') .'/css/jquery.qtip.min.css');
	drupal_add_js(drupal_get_path('module', 'game_war') .'/js/jquery.qtip.min.js');

	if(strpos($_SERVER['HTTP_HOST'], 'localhost') !== FALSE) {
		$path_tmp = variable_get('file_temporary_path' , '');
		if(strpos($path_tmp, 'wamp') === FALSE) {
			variable_set('file_temporary_path', 'C:\wamp\tmp');
		}
	}
}

