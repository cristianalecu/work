function game_war_get_users_on_page($count) {
global $users_on_page;

	if(isset($users_on_page))
	  return;
	
	game_war_add_users_gamedata();
	
	$sSql  = "SELECT u.name, g.* from {users} u, {user_gamedata} g where u.uid=g.uid "; //and pagina = (SELECT pagina from {user_gamedata} where pozitie=$count) ";
	$result = db_query($sSql);
	foreach ($result as $rs) {
		$users_on_page[$rs->pozitie] = (array) $rs;
	}		
}

function game_war_add_users_gamedata() {
global $game_settings;

	$sSql = "select cu, gu from (SELECT count(*) cu FROM {users} WHERE uid>1) u, (SELECT count(*) gu FROM {user_gamedata}) g";
	$rs = db_query($sSql)->fetchAssoc();
	if($rs['cu'] > $rs['gu'])
	{
		$sSql  = "SELECT uid, name from {users} where uid>1 and uid not in (select uid from {user_gamedata}) ";
		$result = db_query($sSql);
		game_war_get_gamesettings();
		foreach ($result as $rs) {
			game_war_insert_userdata($rs->uid, $rs->name);
		}
		game_war_refresh_positions();
	}
}

