}

function game_war_get_userstatus($ar) {
	$args = explode("/", $_GET['q']);
	$output = Array();
	foreach($args as $id => $arg) {
		if($id == 0)
			continue;
	  $params = explode("_" , $arg);

	  $text = NULL;
	   
	  $link = NULL; //game_war_get_userst(intval($params[0]), intval($params[1]), intval($params[3]), intval($params[4]), intval($params[5]), $text);
	  $output[] = array($link, $text, $params[2] . "_" . $params[3] . "_" . $params[4] . "_" . $params[5]);

	}
	drupal_json_output($output);
	exit;
}

/**
 * Inform Ultimate Cron about cron jobs.
 *
 * Note that the result of this hook is cached.
 *
 * @return array
 *   Array of cron jobs, keyed by name.
 *    - "title": (optional) The title of the cron job. If not provided, the
 *      name of the cron job will be used.
 *    - "file": (optional) The file where the callback lives.
 *    - "module": The module where this job lives.
 *    - "file path": (optional) The path to the directory containing the file
 *      specified in "file". This defaults to the path to the module
 *      implementing the hook.
 *    - "callback": (optional) The callback to call when running the job.
 *      Defaults to the job name.
 *    - "callback arguments": (optional) Arguments for the callback. Defaults
 *      to array().
 *    - "enabled": (optional) Initial state of the job. Defaults to TRUE.
 *    - "tags": (optional) Tags for the job. Defaults to array().
 *    - "settings": (optional) Default settings (plugin type) for this job.
 *      Example of a job declaring some default settings for a plugin called
 *      "some_plugin":
 *      'settings' => array(
 *        'some_plugin' => array(
 *          'some_value' => 60,
 *        ),
 *      ),
 *    - "scheduler": (optional) Default scheduler (plugin type) for this job.
 *      Example of a job using the crontab scheduler as default:
 *      'scheduler' => array(
 *        'name' => 'crontab',
 *        'crontab' => array(
 *          'rules' => array('* * * * *'),
 *        ),
 *      ),
 *    - "launcher": (optional) Default launcher (plugin type) for this job.
 *      Example of a job using the serial launcher as default:
 *      'launcher' => array(
 *        'name' => 'serial',
 *        'serial' => array(
 *          'thread' => 'any',
 *        ),
 *      ),
 *    - "logger": (optional) Default logger (plugin type) for this job.
 *      Example of a job using the cache logger as default:
 *      'logger' => array(
 *        'name' => 'cache',
 *        'cache' => array(
 *          'bin' => 'mycachebin',
 *        ),
 *      ),
 */
function game_war_cronapi() {
	$items = array();

	$items['game_war_cron_job_1'] = array(
			'title' => t('War Game Cron main '),
			'callback' => 'game_war_cron_job_1_callback',
			'callback arguments' => array('cronjob1'),
			'enabled' => FALSE,
	);

	return $items;
}

/**
 * The callback for the cron job,
 */
function game_war_cron_job_1_callback($job) {
global $user;
global $game_settings;

    $usr = $user->uid;
    if($usr == 0)
    	$usr = -1;
    game_war_get_gamesettings();
    $dt = date("Y-m-d H:i:s");
    $sSql = "update {war_cron} set uid=$usr where uid=0 and vremea<'$dt';";
    db_query($sSql);
    $sSql  = "SELECT * from {war_cron} where uid=$usr order by tip "; 
    $result = db_query($sSql);
    foreach ($result as $rs) {
    	if($rs->tip == 1) {
    		$sSql = "update {user_gamedata} set atacuri_bonus=case when atacuri_bonus_expire<='$dt' then 0 else atacuri_bonus end " .
    		 ", atacuri_max_bonus=case when atacuri_max_expire<='$dt' then 0 else atacuri_max_bonus end" .
    		 ", vot_bonusuri=case when vot_bon_expire<='$dt' then 0 else vot_bonusuri end" .
    		 ", seif_bonus=case when seif_bonus_expire<='$dt' then 0 else seif_bonus end";
    		db_query($sSql);
    	}
    	else if($rs->tip == 2) {
    		$sSql = "update {user_gamedata} set voturiph=1, seif_extras=0";
    		db_query($sSql);
    	}
    	else if($rs->tip == 3) {
    		$sSql = "update {user_gamedata} set seif_depuneri=case when seif_depuneri<" . $game_settings['seif_depuneri'] . " then " . $game_settings['seif_depuneri'] . " else seif_depuneri end, " .
    		  " atac_dublu_last_used='" . date("Y-m-d H:i:s",strtotime($rs->vremea  ." - 1 minute")) . "'";
    		db_query($sSql);
    	}
    	else if($rs->tip == 10) {
    		$sSql = "update {user_gamedata} set soldati=soldati+soldatiph ";
    		db_query($sSql);
    		game_war_refresh_positions();
    	}
    	else if($rs->tip == 20) {
    		$sSql = "update {user_gamedata} set bani=bani +soldati * " . $game_settings['bani_pe_soldat'];
    		db_query($sSql);
    	}
    	
    	if($rs->xvalue == 0)
    		$sSql = "update {war_cron} set vremea='" . date("Y-m-d H:i:s",strtotime($dt  ." + 1 minute")) . "' where tip=$rs->tip";  //uid=0 and
    	else 
    		$sSql = "update {war_cron} set vremea='" . date("Y-m-d H:i:s",strtotime($rs->vremea  ." + $rs->xvalue minutes")) . "' where tip=$rs->tip";  //uid=0 and
    		    
    	db_query($sSql);
    }
    $sSql = "update {war_cron} set uid=0 where uid=$usr";
    db_query($sSql);
	$sSql = "INSERT INTO {war_cronhist}  VALUES (CURRENT_TIMESTAMP, '1', $usr, '2016-11-11 00:00:00');";
	db_query($sSql);	
	$test = 0;
}

